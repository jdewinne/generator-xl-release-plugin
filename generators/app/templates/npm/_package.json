{
  "name": "<%= kebabPluginName %>",
  "version": "5.0.0",
  "description": "Descrption for <%= pluginName %>",
  "devDependencies": {
    <% if (testFrameworks.length > 0) {%>
      "angular": "1.4.8",
      "angular-mocks": "1.4.8",
    <% } %>
    <% if (testFrameworks.indexOf('karma') > -1) { %>
      "jasmine-core": "2.4.1",
      "karma": "0.13.21",
      "karma-babel-preprocessor": "^6.0.1",
      "karma-jasmine": "0.3.5",
      "karma-phantomjs-launcher": "1.0.0",
      "phantomjs-prebuilt": "2.1.3"
    <% } %>
  },
  "scripts": {
    <% if (testFrameworks.length > 0) { %>
      "test": "karma start karma.conf.js --single-run",
      "watch-test": "karma start karma.conf.js"
    <% } %>
  },
  "private": true
}
